{
  "staticFileGlobs": [
    "public/fonts/lato-light-webfont.woff",
    "public/fonts/lato-light-webfont.woff2",
    "public/fonts/lato-regular-webfont.woff",
    "public/fonts/lato-regular-webfont.woff2",
    "public/build/js/final-*.js",
    "public/build/css/final-*.css",
    "public/img/**/*.{png,jpg,gif,svg}"
  ],
  "stripPrefix": "public",
  "maximumFileSizeToCacheInBytes": 5242880,
  "runtimeCaching": [{
    "urlPattern": "'/'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'https://fonts.googleapis.com/'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'https://fonts.gstatic.com/'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'https://ghbtns.com/'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/themes'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/countries'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/sources'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/years'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/categories'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/groups'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/types'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/languages'",
    "handler": "cacheFirst"
  }, {
    "urlPattern": "'/api/instruments'",
    "handler": "cacheFirst"
  }
],
  "handleFetch": true
}
